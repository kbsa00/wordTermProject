{
    "compilerOptions": {
      "target": "es5",                          
      "module": "commonjs",                    
      "lib": ["es6"],                     
      "allowJs": true,
      "outDir": "build",                          
      "rootDir": "src",
      "strict": true,         
      "noImplicitAny": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
    },
    "include": [
      "src/swagger-output.json",
      "src/**/*"
, "src/swaggerdef.js"    ],
    "exclude": [
      "node_modules"
    ],
    "lib": [
      "es2015"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "express"
    ]
}